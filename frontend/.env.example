# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
REACT_APP_API_BASE_URL=https://api.menuiq.io
REACT_APP_API_URL=https://api.menuiq.io

# Auth0 Configuration (if using Auth0)
# REACT_APP_AUTH0_DOMAIN=your-tenant.auth0.com
# REACT_APP_AUTH0_CLIENT_ID=your-client-id
# REACT_APP_AUTH0_AUDIENCE=https://api.menuiq.io
# REACT_APP_AUTH0_REDIRECT_URI=https://app.menuiq.io/callback

# Application Settings
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=2.0.0

# Public URLs
REACT_APP_PUBLIC_URL=https://menuiq.io
REACT_APP_CDN_URL=https://cdn.menuiq.io

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_FLOWIQ=true
REACT_APP_ENABLE_MULTI_ITEM=true

# Google Analytics (optional)
# REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X

# Sentry Error Tracking (optional)
# REACT_APP_SENTRY_DSN=https://your-sentry-dsn

# Social Media (optional)
# REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
# REACT_APP_INSTAGRAM_CLIENT_ID=your-instagram-client-id

# Map Integration (optional)
# REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# For local development, use:
# REACT_APP_API_URL=http://localhost:8000
# REACT_APP_AUTH0_REDIRECT_URI=http://localhost:3000/callback